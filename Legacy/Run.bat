@PowerShell -Command "$source = \"\"; Get-Content -Encoding \"utf8\" -Path \".\\Source.cs\" | ForEach-Object { $source = $source + $_ + \"`n\" }; $assemblyNames = [System.Collections.ArrayList]@(); ([regex]\"\/\/\/\s*import\s+([\w\.]+)\s*;\").Matches($source) | ForEach-Object {$assemblyNames.Add($_.Groups[1].Value.Trim())} | Out-Null; Add-Type -ReferencedAssemblies $assemblyNames.ToArray() -TypeDefinition \"$source\"; [Program]::Main()"